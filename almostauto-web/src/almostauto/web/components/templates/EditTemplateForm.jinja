{#def template #}

<form hx-put="/templates/{{ template.id }}" hx-target="#template" hx-ext="json-enc">
    <label for="title" class="label">Title</label>
    <input name="title" type="text" value="{{ template.title }}" class="input-bordered">
    <button class="btn btn-primary" type="submit">Save</button>
</form>
